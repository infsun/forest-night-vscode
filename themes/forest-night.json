        // Syntax{{{
        {
            "name": "Preproc",
            "scope": "keyword.control.at-rule, keyword.control.import, keyword.control.export, storage.type.namespace, punctuation.decorator, keyword.control.directive.include, keyword.preprocessor, punctuation.definition.preprocessor, punctuation.definition.directive",
            "settings": {
                "foreground": "#ff9094"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword, support.type.object, storage.type.function, constant.language, keyword.operator.new, storage.type.class",
            "settings": {
                "foreground": "#ff9094"
            }
        },
        {
            "name": "Storage",
            "scope": "storage, modifier, keyword.var, entity.name.tag",
            "settings": {
                "foreground": "#ecaf93"
            }
        },
        {
            "name": "Class",
            "scope": "entity.name.type.class, support.class, entity.other.inherited-class, entity.other.attribute-name",
            "settings": {
                "foreground": "#eccc8f"
            }
        },
        {
            "name": "Modules",
            "scope": "support.module, support.node",
            "settings": {
                "foreground": "#eccc8f"
            }
        },
        {
            "name": "Type",
            "scope": "support.type, entity.name.type, storage.type.built-in.primitive, storage.type.built-in, storage.type.primitive, keyword.type",
            "settings": {
                "foreground": "#eccc8f"
            }
        },
        {
            "name": "String Escape",
            "scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
            "settings": {
                "foreground": "#eccc8f"
            }
        },
        {
            "name": "Enum",
            "scope": "entity.name.type.enum",
            "settings": {
                "foreground": "#eccc8f"
            }
        },
        {
            "name": "Function",
            "scope": "entity.name.function, support.function, meta.function, entity.name.function, meta.definition.method, meta.function-call.generic",
            "settings": {
                "foreground": "#b9ce8e"
            }
        },
        {
            "name": "String",
            "scope": "string, punctuation.definition.string.end, punctuation.definition.string.begin",
            "settings": {
                "foreground": "#b9ce8e"
            }
        },
        {
            "name": "Unit",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#b9ce8e"
            }
        },
        {
            "name": "Macro",
            "scope": "entity.name.function.preprocessor",
            "settings": {
                "foreground": "#a2cea5"
            }
        },
        {
            "name": "Property",
            "scope": "variable.object.property, support.variable.property, variable.other.property, variable.other.object.property.js",
            "settings": {
                "foreground": "#a2cea5"
            }
        },
        {
            "name": "Operator",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#bdd0e5"
            }
        },
        {
            "name": "Debug",
            "scope": "keyword.other.debugger",
            "settings": {
                "foreground": "#e5c6f0"
            }
        },
        {
            "name": "Special identifier",
            "scope": "variable.language.this, variable.language.super",
            "settings": {
                "foreground": "#e5c6f0"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#e5c6f0"
            }
        },
        {
            "name": "Constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#e5c6f0"
            }
        },
        {
            "name": "Identifier",
            "scope": "variable, support.variable, support.constant, meta.definition.variable, variable.other.enummember",
            "settings": {
                "foreground": "#f6e3bc"
            }
        },
        {
            "name": "Delimiter",
            "scope": "punctuation, meta.brace",
            "settings": {
                "foreground": "#f6e3bc"
            }
        },
        {
            "name": "Comment",
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "foreground": "#7f8f9f",
                "fontStyle": "italic"
            }
        }
        // }}}
